{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": ["ES6", "DOM"],
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "strict": true,
        "target": "ES6"
    }
}
